{
  "name": "bundlephobia",
  "version": "1.0.0",
  "main": "./build/index.js",
  "repository": "https://github.com/mbriziarelli/bundlephobia.git",
  "author": "Maxime Briziarelli <maxime.briziarelli@gmail.com>",
  "license": "UNLICENSED",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "dev": "NODE_ENV=development yarn build:server && node .",
    "start": "NODE_ENV=production node .",
    "build": "yarn build:server && yarn build:next",
    "build:server": "tsc --project tsconfig.buildServer.json",
    "build:next": "next build",
    "test": "yarn test:lint && yarn test:unit",
    "test:lint": "eslint --report-unused-disable-directives --ext .ts ./pages ./server ./typings",
    "test:unit": "jest"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "express": "^4.17.1",
    "jest-worker": "^25.1.0",
    "next": "^9.2.2",
    "package-build-stats": "^6.1.2",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "tslib": "^1.11.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.3",
    "@types/jest": "^25.1.4",
    "@types/node": "^13.9.0",
    "@types/react": "^16.9.23",
    "@types/react-dom": "^16.9.5",
    "@typescript-eslint/eslint-plugin": "^2.22.0",
    "@typescript-eslint/parser": "^2.22.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-prettier": "^3.1.2",
    "jest": "^25.1.0",
    "prettier": "^1.19.1",
    "ts-jest": "^25.2.1",
    "typescript": "^3.8.3"
  }
}
