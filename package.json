{
  "name": "bundlephobia",
  "version": "1.0.0",
  "main": "./build/index.js",
  "repository": "https://github.com/mbriziarelli/bundlephobia.git",
  "author": "Maxime Briziarelli <maxime.briziarelli@gmail.com>",
  "license": "UNLICENSED",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "dev": "NODE_ENV=development yarn build:server && node .",
    "start": "NODE_ENV=production node .",
    "build": "yarn build:server && yarn build:next",
    "build:server": "tsc --project tsconfig.buildServer.json",
    "build:next": "next build",
    "test": "yarn test:lint && yarn test:unit",
    "test:lint": "eslint --report-unused-disable-directives --ext .ts,.tsx,.json ./__data__ ./__mocks__ ./.storybook ./components ./pages ./server ./typings ./utils",
    "test:unit": "jest",
    "storybook": "start-storybook"
  },
  "dependencies": {
    "@material-ui/core": "^4.9.7",
    "@material-ui/lab": "^4.0.0-alpha.46",
    "axios": "^0.19.2",
    "express": "^4.17.1",
    "jest-worker": "^25.1.0",
    "next": "^11.1.3",
    "package-build-stats": "^6.1.2",
    "pacote": "^11.1.4",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "semver": "^7.1.3",
    "tslib": "^1.11.1",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^5.3.17",
    "@storybook/addon-info": "^5.3.17",
    "@storybook/cli": "^5.3.17",
    "@storybook/react": "^5.3.17",
    "@testing-library/dom": "^7.0.4",
    "@testing-library/react": "^10.0.1",
    "@types/express": "^4.17.3",
    "@types/jest": "^25.1.4",
    "@types/node": "^13.9.2",
    "@types/pacote": "^9.5.0",
    "@types/react": "^16.9.23",
    "@types/react-dom": "^16.9.5",
    "@types/semver": "^7.1.0",
    "@types/supertest": "^2.0.8",
    "@types/testing-library__dom": "^6.14.0",
    "@types/testing-library__react": "^9.1.3",
    "@typescript-eslint/eslint-plugin": "^2.24.0",
    "@typescript-eslint/parser": "^2.24.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.19.0",
    "jest": "^25.1.0",
    "prettier": "^1.19.1",
    "react-docgen-typescript-loader": "^3.7.1",
    "supertest": "^4.0.2",
    "ts-jest": "^25.2.1",
    "ts-loader": "^6.2.1",
    "typescript": "^3.8.3"
  }
}
